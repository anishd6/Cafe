<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="mainPane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="560.0" prefWidth="850.0" xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="application.ShellController">
	<left>
		<AnchorPane prefHeight="200.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<children>
				<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#a67b5b"
					height="560.0" stroke="TRANSPARENT" strokeType="INSIDE"
					width="200.0" />
				<Text fx:id="dashboardNav" fill="WHITE" layoutX="69.0"
					layoutY="134.0" onMouseClicked="#homeBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Home">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Text>
				<Text fx:id="settingsNav" fill="WHITE" layoutX="71.0"
					layoutY="177.0" onMouseClicked="#menuBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Menu">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Text>
				<Text fx:id="logOutBtn" fill="WHITE" layoutX="57.0"
					layoutY="226.0" onMouseClicked="#nutritionBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Nutrition">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Text>
				<Text fx:id="logOutBtn1" fill="WHITE" layoutX="60.0"
					layoutY="273.0" onMouseClicked="#couponBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Coupon">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Text>
				<Text fx:id="logOutBtn2" fill="WHITE" layoutX="45.0"
					layoutY="324.0" onMouseClicked="#orderNowBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Order Now">
					<font>
						<Font name="System Bold" size="24.0" />
					</font>
				</Text>
			</children>
		</AnchorPane>
	</left>
</BorderPane>
